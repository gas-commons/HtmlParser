{
  "name": "HtmlParser",
  "scripts": {
    "start": "npm run watch",
    "login": "clasp login",
    "logout": "clasp logout",
    "create": "clasp create --type standalone --title HtmlParser --rootDir src",
    "push": "clasp push",
    "watch": "clasp push --watch",
    "open": "clasp open",
    "lint": "eslint --ext .ts --ignore-path .gitignore src",
    "fix": "eslint --fix --ext .ts --ignore-path .gitignore src"
  },
  "devDependencies": {
    "@google/clasp": "^2.2.1",
    "@types/google-apps-script": "0.0.56",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "eslint": "^6.2.1",
    "eslint-config-standard": "^14.0.1",
    "eslint-plugin-googleappsscript": "^1.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  }
}
